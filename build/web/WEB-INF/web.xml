    <servlet>
        <servlet-name>ProcedimentoRemoverRoutes</servlet-name>
        <servlet-class>com.cadastro.routes.ProcedimentoRemoverRoutes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ExameRemoverRoutes</servlet-name>
        <servlet-class>com.cadastro.routes.ExameRemoverRoutes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ExameListRoutes</servlet-name>
        <servlet-class>com.cadastro.routes.ExameListRoutes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ExameRoutes</servlet-name>
        <servlet-class>com.cadastro.routes.ExameRoutes</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PacienteRoutes</servlet-name>
        <url-pattern>/paciente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PacienteListRoutes</servlet-name>
        <url-pattern>/paciente/list</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PacienteRemoverRoutes</servlet-name>
        <url-pattern>/paciente/remover</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcedimentoRoutes</servlet-name>
        <url-pattern>/procedimento</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcedimentoListRoutes</servlet-name>
        <url-pattern>/procedimento/list</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcedimentoRemoverRoutes</servlet-name>
        <url-pattern>/procedimento/remove</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ExameRemoverRoutes</servlet-name>
        <url-pattern>/exame/remover</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ExameListRoutes</servlet-name>
        <url-pattern>/exame/list</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ExameRoutes</servlet-name>
        <url-pattern>/exame</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
